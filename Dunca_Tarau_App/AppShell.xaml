<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Dunca_Tarau_App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Dunca_Tarau_App"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
 Title="Welcome"
 ContentTemplate="{DataTemplate local:MainPage}"
 />

        <ShellContent
 Title="My tour Lists"
 ContentTemplate="{DataTemplate local:ListEntryPage}"
 />
        <ShellContent
 Title="Countries"
 ContentTemplate="{DataTemplate local:CountryEntryPage}"
 />
        <ShellContent
 Title="Categories"
 ContentTemplate="{DataTemplate local:TourCategoryEntryPage}"
 />
        <ShellContent
 Title="About"
 ContentTemplate="{DataTemplate local:AboutPage}"
 />
    </TabBar>

</Shell>
